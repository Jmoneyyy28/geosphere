{
  "expo": {
    "name": "geosphere",
    "slug": "geosphere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "geosphere",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptiveIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.teamgeo.geosphere"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@viro-community/react-viro"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "entryPoint": "./app/login.tsx",
    "extra": {
      "eas": {
        "projectId": "59f3ffb6-eb87-4db1-a5a0-430246ec248a"
      }
    }
  }
}
